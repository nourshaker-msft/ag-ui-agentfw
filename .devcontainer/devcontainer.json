{
	"name": "AI Gateway Dev Container",
	"image": "mcr.microsoft.com/devcontainers/universal:5.0",
	"hostRequirements": {
	  "cpus": 4
	},
	"features": {
		"ghcr.io/devcontainers/features/azure-cli:1": {
			"installBicep": true,
			"version": "latest"
		}
	},
	"waitFor": "onCreateCommand",
    "updateContentCommand": "bash setup.sh",
    "postCreateCommand": "",
	"customizations": {
	  "codespaces": {
		"openFiles": []
	  },
	  "vscode": {
		"extensions": [
		  "ms-toolsai.jupyter",
		  "ms-python.python"
		]
	  }
	}
}
